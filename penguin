	echo -e "\x1b[1;31m\x20\x20\x20\x20\x20\x20\x1b[1;38;5;178m\x5e\x1b[1;38;5;234m\x3a\x1b[1;38;5;178m\x5e\x20\x20\x20\x20\x1b[1;38;5;236m\x3a\x3a\x3a\x3a\x3a\x1b[1;38;5;178m\x5e\x1b[1;38;5;240m\x3a\x3a\x3a\x3a\x3a\x1b[1;38;5;242m\x2e\x3a\x3a\x3a\x1b[1;38;5;178m\x5e\x20\x1b[1;38;5;178m\x5e\x1b[1;38;5;248m\x3a\x3a\x3a\x1b[1;38;5;178m\x5e\x1b[1;38;5;240m\x3a\x3a\x3a\x3a\x3a\x1b[1;38;5;178m\x5e\x1b[1;38;5;240m\x2e\x20\x20\x20\x20\x20\x20\x20\x20"
	echo -e "\x1b[1;31m\x20\x20\x20\x20\x20\x20\x20\x1b[1;38;5;178m\x5e\x20\x20\x20\x20\x5e\x1b[1;38;5;236m\x3a\x3a\x3a\x3a\x3a\x20\x1b[1;38;5;178m\x5e\x1b[1;38;5;240m\x3a\x3a\x1b[1;38;5;178m\x5e\x20\x1b[1;38;5;242m\x2e\x3a\x3a\x3a\x1b[1;38;5;178m\x5e\x20\x1b[1;38;5;248m\x2e\x1b[1;38;5;178m\x5e\x1b[1;38;5;248m\x3a\x3a\x3a\x1b[1;38;5;236m\x3a\x3a\x1b[1;38;5;240m\x3a\x3a\x3a\x3a\x3a\x1b[1;38;5;178m\x5e\x1b[1;38;5;240m\x2e\x20\x20\x20\x20\x20\x20"
	echo -e "\x1b[1;31m\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x1b[1;38;5;178m\x5e\x1b[1;38;5;236m\x3a\x3a\x3a\x3a\x3a\x20\x20\x20\x20\x20\x20\x20\x1b[1;38;5;242m\x3a\x3a\x3a\x2e\x20\x20\x1b[1;38;5;178m\x5e\x1b[1;38;5;248m\x3a\x3a\x3a\x1b[1;38;5;236m\x3a\x3a\x3a\x1b[1;38;5;240m\x3a\x3a\x3a\x3a\x3a\x3a\x1b[1;38;5;178m\x5e\x1b[1;38;5;240m\x2e\x20\x20\x20\x20"
	echo -e "\x1b[1;31m\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x1b[1;38;5;236m\x3a\x3a\x3a\x3a\x3a\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x1b[1;38;5;248m\x2e\x2e\x20\x1b[1;38;5;236m\x3a\x3a\x3a\x3a\x1b[1;38;5;240m\x3a\x3a\x3a\x3a\x1b[1;38;5;178m\x5e\x20\x20\x20\x20\x20\x20"
	echo -e "\x1b[1;31m\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x1b[1;38;5;236m\x3a\x3a\x3a\x3a\x3a\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x1b[1;38;5;236m\x3a\x3a\x3a\x3a\x3a\x2e\x20\x20\x20\x20\x20\x20\x20\x20\x20"
	echo -e "\x1b[1;31m\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x1b[1;38;5;236m\x3a\x3a\x3a\x3a\x3a\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20\x1b[1;38;5;236m\x3a\x3a\x3a\x3a\x3a\x20\x20\x20\x20\x20\x20\x20\x20\x20\x20"
