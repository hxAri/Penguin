							wget -qO- --tries=20 ${rinku[0]}/vncserver-stop --show-progress --progress=bar:force:noscroll -O /usr/local/bin/vncserver-stop
							
							echo -e "$(stdio stdout $distro chmod+x vncviewer-stop)"
							chmod +x /usr/local/bin/vncserver-stop
						fi
