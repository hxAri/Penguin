							local rinku=(
								"https://raw.githubusercontent.com/AndronixApp/AndronixOrigin/master/APT/LXDE"
								"lxde_de.sh"
							)
						;;
					esac
					
					echo "$(stdio stdout $distro desktop setup)"
					echo "APT::Acquire::Retries \"3\";" > $source/$folder/etc/apt/apt.conf.d/80-retries
					
