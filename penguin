							pacman -S tigervnc --noconfirm > /dev/null
						fi
						clear
						
						echo -e "$(stdio stdout $distro removing ${desktop}.sh)"
						rm -rf /root/${desktop}.sh
						
						echo -e "$(stdio stdout $distro removing .bash_profile)"
						rm -rf /root/.bash_profile
						
